<Window x:Class="Кредитный_калькулятор.toFormat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Кредитный_калькулятор"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Сохранение файла" Height="200" Width="200">
    <Grid Margin="0,0,0,0">
        <Label x:Name="iFile" Content="Формат файла" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="178"/>
        <ComboBox Name="formatFile" Height="20" Width="180" Margin="0,40,0,0" VerticalAlignment="Top">
            <TextBlock Name="json">json</TextBlock>
            <TextBlock Name="pdf">pdf</TextBlock>
        </ComboBox>
        <Label x:Name="itsrange" Content="Название файла" HorizontalAlignment="Center" Margin="0,64,0,0" VerticalAlignment="Top" Width="178"/>
        <TextBox x:Name="nameFile" HorizontalAlignment="Center" Margin="0,95,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="178"/>
        <Button x:Name="button" Content="Создать" Click="CreateFile" HorizontalAlignment="Center" Margin="0,125,0,0" VerticalAlignment="Top" Width="178" Height="27"/>
    </Grid>
</Window>